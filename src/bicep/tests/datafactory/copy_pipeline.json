{
    "name": "copy_pipeline",
    "properties": {
        "activities": [
            {
                "name": "Copy raw to processed",
                "type": "Copy",
                "dependsOn": [],
                "policy": {
                    "timeout": "7.00:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "source": {
                        "type": "BinarySource",
                        "storeSettings": {
                            "type": "AzureBlobFSReadSettings",
                            "recursive": true,
                            "deleteFilesAfterCompletion": true
                        },
                        "formatSettings": {
                            "type": "BinaryReadSettings"
                        }
                    },
                    "sink": {
                        "type": "BinarySink",
                        "storeSettings": {
                            "type": "AzureBlobFSWriteSettings"
                        }
                    },
                    "enableStaging": false
                },
                "inputs": [
                    {
                        "referenceName": "bin_ds",
                        "type": "DatasetReference",
                        "parameters": {
                            "containerName": "raw"
                        }
                    }
                ],
                "outputs": [
                    {
                        "referenceName": "bin_ds",
                        "type": "DatasetReference",
                        "parameters": {
                            "containerName": "processed"
                        }
                    }
                ]
            }
        ],
        "annotations": []
    }
}